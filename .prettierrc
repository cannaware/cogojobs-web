{
  "parser": "typescript",
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^@/components/(.*)$",
    "^@/hooks/(.*)$",
    "^@/interfaces/(.*)$",
    "^@/utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
